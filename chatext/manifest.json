{
	"manifest_version": 3,
	"name": "ChatExt",
	"description": "An extension that allows users to upload files to chatGPT",
	"version": "1.0.0",
	"author": "Bilal Masters",
	"permissions": ["activeTab"],
	"action": {
		"default_popup": "index.html"
	},
	"content_scripts": [
		{
			"matches": ["https://*/*"],
			"js": ["content.js"]
		}
	],
	"web_accessible_resources": []
}
