{
	"manifest_version": 3,
	"name": "ChatExt",
	"description": "An extension that allows users to upload files to chatGPT",
	"version": "1.0.0",
	"author": "Bilal Masters",
	"permissions": ["activeTab"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"web_accessible_resources": [
		{
			"resources": ["ChatExtContainer.js"],
			"matches": ["<all_urls>"]
		}
	]
}
